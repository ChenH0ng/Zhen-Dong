{
  "manifest_version": 2,
  "name": "zhen dong",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "zhen dong"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "background",
    "*://*/*"
  ],
  "background": {
//    "page": "background.html",
    "persistent": true,
    "scripts":["eventPage.js"]
  }
}